{
  "name": "rvs.hu",
  "version": "0.0.1",
  "description": "rvs.hu rewritten in React.js",
  "main": "",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "browserify": "mkdir -p ./dist && browserify src/index.jsx -o dist/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "copy:dist": "cp src/*.html dist/ && cp src/*.css dist/",
    "copy": "npm run copy:dist",
    "bundle": "watch 'npm run browserify && npm run copy && npm test' ./src ./test",
    "start": "http-server dist/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-istanbul": "^2.0.0",
    "fast-async": "^6.0.20",
    "http-server": "^0.9.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "git@github.com:szilveszter9/karma-coverage#e71ff2f170a86b",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "mocha": "^2.4.5",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "watch": "^0.18.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {}
}
